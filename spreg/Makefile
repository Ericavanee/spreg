	find . -name "*.py" -print | xargs sed -i -- 's/import pysal/import libpysal\.api as lps/g'
	find . -name "*.py" -print | xargs sed -i -- 's/pysal\.open()/lps\.open()/g'
	find . -name "*.py" -print | xargs sed -i -- 's/pysal\.spreg/spreg/g'
